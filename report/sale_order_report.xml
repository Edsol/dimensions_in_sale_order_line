<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="view_sale_order_line_inherit_dimensions" inherit_id="sale.report_saleorder_document">
        <xpath expr="//table[1]/thead/tr[1]/th[1]" position="before">
            <th class="text-left">Width</th>
        </xpath>
        <xpath expr="//table[1]/tbody/t[2]/tr[1]/t[1]/td[1]" position="before">
            <td class="text-left">
                <span t-field="line.width"/>
            </td>
        </xpath>
    </template>
</odoo>
